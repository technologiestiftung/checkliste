{
	"framework": "react-router",
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	],
	"trailingSlash": true,
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "s-maxage=1, stale-while-revalidate=59"
				}
			]
		}
	]
}
